@model JobTask.ServiceLayer.Pension.PensionTinModel

<form action="/Home/get-tin" method="get">
    <input type="text" name="pinfl" placeholder="Pinfl" />
    <input type="submit" value="Get" />
    <br />
    @if (Model != null) 
        {
            <h2> Tin: @Model.Data.Tin.ToString() </h2>
        }
    else
        {
            <h2> Mavjud emas! </h2>
        };

</form>
